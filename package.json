{
  "name": "devops-knowledgeops-agent",
  "version": "1.0.0",
  "description": "AI-powered DevOps assistant using Amazon Bedrock AgentCore",
  "private": true,
  "workspaces": [
    "backend",
    "frontend",
    "lambda/*"
  ],
  "scripts": {
    "start": "cd backend && npm start",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "start:dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "deploy": "cd infrastructure && cdk deploy",
    "destroy": "cd infrastructure && cdk destroy",
    "setup": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules lambda/*/node_modules",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint"
  },
  "keywords": [
    "devops",
    "ai",
    "bedrock",
    "agentcore",
    "aws",
    "hackathon"
  ],
  "author": "DevOps Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "node-fetch": "^3.3.2"
  }
}
